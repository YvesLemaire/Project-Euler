   "source": [
    "from projectEuler import sumDivisors\n",
    "\n",
    "d = lambda n: sumDivisors(n) - n\n",
    "\n",
    "# on peut v√©rifier qu'il n'y a pas de couple amical avec a < 10000 <= b\n",
    "s = 0\n",
    "for a in range(2, 10000):\n",
    "    b = d(a)\n",
    "    if b > a and d(b) == a:\n",
    "        s += a + b\n",
    "s"
   ]
  }
