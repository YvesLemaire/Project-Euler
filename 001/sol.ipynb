    "def arithmeticSum(p):\n",
    "    # -> la somme des multiples de p < a 1000\n",
    "    n = 999 // p\n",
    "    return p * n * (n + 1) // 2\n",
    "\n",
    "arithmeticSum(3) + arithmeticSum(5) - arithmeticSum(15)"
   ]
  }
