    "def f(n):\n",
    "    return n * f(n - 1) if n else 1\n",
    "\n",
    "def sumDigits(n):\n",
    "    q, r = n // 10, n % 10\n",
    "    return r + sumDigits(q) if q else r\n",
    "\n",
    "sumDigits(f(100))"
   ]
